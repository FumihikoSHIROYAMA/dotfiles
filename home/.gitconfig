[user]
    name = Fumihiko Shiroyama
    email = f.shiroyama@mana.bo
[core]
    editor = vim -c \"set fenc=utf-8\"
    pager = lv -c
[color]
    ui = true
[alias]
    co = checkout
    st = status
    ci = commit
    df = diff
    br = branch
    ad = add
    lo = log
    ps = push

[remote "pr"]
    url = git@github.com:manabo-inc/mana.bo_android.git
    fetch = +refs/pull/*:refs/remotes/pr/*
